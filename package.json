		"autoprefixer": "^10.4.14",
		"flowbite-svelte-icons": "^0.3.6",
		"husky": "^8.0.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"pretty-quick": "^3.1.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-intersection-observer": "^0.10.1",
